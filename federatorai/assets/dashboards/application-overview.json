        {
            "title_size": 16,
            "title": true,
            "title_align": "left",
            "title_text": "Current/Predicted CPU Usage (Daily)",
            "height": 15,
            "tile_def": {
                "viz": "timeseries",
                "requests": [
                    {
                        "style": {
                            "palette": "dog_classic",
                            "width": "normal",
                            "type": "solid"
                        },
                        "q": "sum:kubernetes.cpu.usage.total{$kube_cluster,$kube_namespace,$kube_deployment,$kube_stateful_set}/1000000",
                        "on_right_yaxis": false,
                        "type": "line",
