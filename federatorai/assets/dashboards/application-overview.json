                            "sum:kubernetes.cpu.usage.total{$kube_cluster,$kube_namespace,$kube_deployment,$kube_stateful_set}/1000000": {
                                "alias": "kubernetes.cpu.usage.total_millicores"
                            }
                        }
                    },
                    {
                        "q": "max:federatorai.prediction.controller{$kube_cluster,$kube_namespace,$kube_deployment,source_metric:kubernetes.cpu.usage.total,prediction_window:24_hours,$kube_stateful_set}",
                        "on_right_yaxis": false,
                        "style": {
                            "palette": "dog_classic",
                            "width": "thick",
                            "type": "dotted"
                        },
                        "type": "line"
